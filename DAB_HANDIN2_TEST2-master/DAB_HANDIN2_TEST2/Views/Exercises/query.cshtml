@using DAB_HANDIN2_TEST2.Controllers
@using DAB2.data
@model IEnumerable<DAB2.models.Exercise>


@{
    ViewData["Title"] = "query";
}

<h1>query</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.HelpWhere)
            </th>

            <th>
                
                @Html.DisplayNameFor(model => model.Student.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CourseId)   
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.HelpWhere)
            </td>

            <td>

                @Html.DisplayFor(modelItem => item.Student.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CourseId)
            </td>

        </tr>
}
    </tbody>
</table>
